.search{
    width:100%;
    height:80rpx;
    line-height: 80rpx;
    padding-left:20rpx;
    padding-right:20rpx;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    background: #fff;
    border-top: 1rpx solid #F7F7F7;
    position: fixed;
    left:0;
    top:0;
    z-index: 200;
}
.search icon{
    position: relative;
    top: 7rpx;
    left: -3rpx;
    color: rgb(26, 26, 26);
}
.search text{
    color: rgb(173, 173, 173);
}
.nav{
    display: flex;
    flex-direction: column;
    background: rgb(230, 230, 230);
    margin-top: 20rpx;
}
.title{
    font-size: 40rpx;
    text-align: center;
    margin-bottom: 20rpx;
    margin-top: 90rpx;
}
.container{
    margin-top: 30rpx;
    padding-left:90rpx;
    padding-right:90rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
v-book{
    margin-bottom: 60rpx;
}