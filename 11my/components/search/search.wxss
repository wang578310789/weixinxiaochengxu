.content{
    width: 100%;
    height: 100rpx;
    background: rgb(247, 247, 247);
    border-bottom: 1px solid rgb(209, 209, 209);
    border-top: 1px solid rgb(209, 209, 209);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 102;
}  

.search{
    width:530rpx;
    height:70rpx;
    background: rgb(228, 228, 228);
    border-radius:30rpx;
    padding-left: 20rpx;
    margin-top: 15rpx;
    display: flex;
    align-items: center;
}
.search input{
    flex:1;
    margin-left: 20rpx;
}
.search .clear{
    margin-right: 20rpx;
}
.btn{
    width: 120rpx;
    height: 80rpx;
    line-height: 80rpx;
    border: 1px solid rgb(139, 139, 139);
    border-radius: 17px;
    position: relative;
    top: -73rpx;
    left: 270rpx;
    font-size: 26rpx;
}
.container{
    padding: 20rpx;
    margin-top: 100rpx;
}
.title{
    display: flex;
    height:90rpx;
    align-items: center;
}
.line{
    height:40rpx;
    width:10rpx;
    background: #333;
    margin-right: 10rpx;
}
.bot{
    margin-top: 120rpx;
    padding-left:90rpx;
    padding-right:90rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
v-book{
    margin-bottom: 60rpx;
}
.result{
    position: fixed;
    top: 50%;
    text-align: center;
    width: 100%;
}
.scroll-view{
    height: 1300rpx;
}